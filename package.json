{
  "name": "jsonapi-redux-data",
  "version": "1.0.17",
  "description": "Library that makes integration of jsonapi with redux effortless and easy",
  "repository": {
    "type": "git",
    "url": "git://github.com/wednesday-solutions/jsonapi-redux-data.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.15.1"
  },
  "keywords": [
    "jsonapi",
    "redux",
    "json-api",
    "redux-data",
    "reactjs",
    "react",
    "api",
    "apisauce"
  ],
  "author": "Mac",
  "homepage": "https://github.com/wednesday-solutions/jsonapi-redux-data",
  "license": "MIT",
  "main": "dist/jsonapi-redux-data",
  "scripts": {
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .eslintignore",
    "lint:eslint:fix": "eslint --ignore-path .eslintignore --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:staged": "jest --findRelatedTests",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write",
    "webpack:prod": "webpack -p --mode=production",
    "prepublishOnly": "yarn webpack:prod"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force",
      "jest --findRelatedTests $STAGED_FILES"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "apisauce": "^1.1.0",
    "cross-env": "5.2.0",
    "deepmerge": "^4.2.2",
    "immer": "8.0.1",
    "immutable": "^4.0.0-rc.12",
    "lodash": "4.17.11",
    "map-keys-deep": "^0.0.2",
    "pluralize": "^8.0.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "redux": "4.0.1",
    "reduxsauce": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@storybook/react": "^5.2.1",
    "babel-loader": "8.0.5",
    "coveralls": "3.0.3",
    "eslint": "5.16.0",
    "gh-pages": "^2.1.1",
    "jest-cli": "24.7.1",
    "lint-staged": "8.1.5",
    "prettier": "1.17.0",
    "rimraf": "2.6.3",
    "webpack": "4.30.0",
    "webpack-cli": "^3.3.10"
  }
}
